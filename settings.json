    "python.pythonPath": "/Library/Frameworks/Python.framework/Versions/3.7/bin/python3",
    "editor.fontFamily": "Fira Code, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontLigatures": true

    "workbench.colorCustomizations": {

        "editor.background": "#1F0045"
        "sideBar.background": "#36109C"
        "sideBarSectionHeader.background": "#257DFF"
        "scrollbarSlider.background": "#8385F6"
        "tab.activeBackground": "#257DFF"
        "activityBar.background": "#36109C"
        "tab.inactiveBackground": "#1F0045"
        "editorGroupHeader.tabsBackground": "#1F0045"
        "list.hoverBackground": "#FF9801"
        "list.inactiveSelectionBackground": "#00CFA2"
    }

    "editor.tokenColorCustomizations": {
        "comments": "#38E4C0" // radiation_green
        "strings": "#E451C4" // pink
        "variables": "#FD9E03" // orange
        "numbers": "#A6F305" // neon_green
        "functions": "#FFCB00" // yellow
        "types": "#FF5068" // salmon
        "keywords": "#FD77BD" // pale pink
    }
}


